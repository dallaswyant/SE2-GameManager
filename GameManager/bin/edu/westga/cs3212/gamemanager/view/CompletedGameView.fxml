         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Label fx:id="otherPlayers_txt" layoutX="139.0" layoutY="157.0" prefHeight="283.0" prefWidth="216.0">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="otherPlayers_lbl" layoutX="40.0" layoutY="156.0" text="Other Players:" />
      <Label layoutX="72.0" layoutY="133.0" text="Winner:" />
      <Label fx:id="winner_txt" layoutX="139.0" layoutY="133.0" prefHeight="17.0" prefWidth="201.0" />
   </children>
</AnchorPane>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.westga.cs3212.gamemanager.view.CompletedGameViewController">
   <children>
      <Button fx:id="back_btn" layoutX="14.0" layoutY="92.0" mnemonicParsing="false" onAction="#back_clicked" text="Return to Game List" />
